             case 2: document.getElementById('jpdt').style.display = (document.getElementById('jpdt').style.display != "none" ? "none" : ""); break; 
             case 3:
             case 4: document.getElementById('waterInfo').style.display = (document.getElementById('waterInfo').style.display != "none" ? "none" : ""); break;
             case 5: document.getElementById('database').style.display = (document.getElementById('database').style.display != "none" ? "none" : ""); break;
             case 6:
             case 7: window.location.href = "Admin/Login.aspx";
             default: break;
         }
         //alert(document.getElementById('waterInfo').style.display);
