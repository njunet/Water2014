<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<security name="RestrictUploadedImageDimension">true</security>
	<security name="OverWriteExistingUploadedFile">false</security>
	<security name="AutoResizeUploadedImages">true</security>
	<security name="MaxImageWidth">1024</security>
	<security name="MaxImageHeight">768</security>
	<security name="MaxImageSize">2048</security>
	<security name="MaxMediaSize">65535</security>
	<security name="MaxFlashSize">100</security>
	<security name="MaxDocumentSize">10000</security>
	<security name="MaxTemplateSize">10000</security>
	<security name="ImageGalleryPath">~/Document/CuteEditor/Pic/</security>
	<security name="MediaGalleryPath">~/Document/CuteEditor/Media/</security>
	<security name="FlashGalleryPath">~/Document/CuteEditor/Flash</security>
	<security name="TemplateGalleryPath">~/Document/</security>
<security name="FilesGalleryPath">~/Document/CuteEditor/Affix/</security>
	<security name="MaxImageFolderSize">102400000</security>
	<security name="MaxMediaFolderSize">102400000</security>
	<security name="MaxFlashFolderSize">102400000</security>
	<security name="MaxDocumentFolderSize">102400000</security>
	<security name="MaxTemplateFolderSize">10240000</security>
 
	<security name="ThumbnailWidth">640</security>
	<security name="ThumbnailHeight">480</security>
	<security name="ThumbnailColumns">5</security>
	<security name="ThumbnailRows">3</security>
	<security name="AllowUpload">true</security>
	<security name="AllowModify">false</security>
	<security name="AllowRename">true</security>
	<security name="AllowDelete">false</security>
	<security name="AllowCopy">false</security>
	<security name="AllowMove">false</security> <!-- this is rename too -->
	<security name="AllowCreateFolder">false</security>
	<security name="AllowDeleteFolder">false</security>
	<security name="ImageFilters">
		<item>.jpg</item>
		<item>.jpeg</item>
		<item>.gif</item>
		<item>.png</item>
	</security>
	<security name="MediaFilters">
		<item>.avi</item>
		<item>.mpg</item>
		<item>.mpeg</item>
		<item>.mp3</item>
	</security>
	<security name="DocumentFilters">
		<item>.txt</item>
		<item>.doc</item>
		<item>.pdf</item>
		<item>.zip</item>
		<item>.rar</item>
		<item>.avi</item>
		<item>.mpg</item>
		<item>.mpeg</item>
		<item>.swf</item>
		<item>.jpg</item>
		<item>.jpeg</item>
		<item>.gif</item>
		<item>.png</item>
		<item>.htm</item>
		<item>.xls</item>
	</security>
</configuration>